class BeautifulInscription{
    function int printInscription(){
        var int tick;
        let tick = 0;
        while(~(Keyboard.keyPressed() = 32)){
            do Output.moveCursor(8,20);
            let tick = tick + 1;
            do Output.printString("Press key space");
            do Sys.wait(100);
        }
        do Output.moveCursor(0,0);
        do Screen.clearScreen();
        return tick;
    }
}