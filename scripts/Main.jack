class Main{
    function void main () {
        var GameField gameField;
        var int ticks;
        let ticks = 0;
        let gameField = GameField.new(481,205,511,255,0,100);
        while (true){
            let ticks = ticks + 1;
            do gameField.MovePipe();
            do Sys.wait(100);
            if (ticks>30){
                do gameField.CreatePipe(481,205,511,255,0,100);
                let ticks = 0;
            }
        }
        return;
    }
}